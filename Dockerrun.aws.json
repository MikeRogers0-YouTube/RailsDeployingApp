{
  "AWSEBDockerrunVersion": "2",
  "containerDefinitions": [
    {
      "name": "server",
      "image": "402239435993.dkr.ecr.eu-west-2.amazonaws.com/rails-deploying-app:latest",
      "memoryReservation": "256",
      "portMappings": [
        {
          "hostPort": "80",
          "containerPort": "3000"
        }
      ],
      "command": ["bundle", "exec", "rails", "s", "-b", "0.0.0.0", "-p", "3000"]
    },
    {
      "name": "sidekiq",
      "image": "402239435993.dkr.ecr.eu-west-2.amazonaws.com/rails-deploying-app:latest",
      "memoryReservation": "256",
      "command": ["bundle", "exec", "sidekiq"]
    }
  ]
}
